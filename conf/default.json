{
    "server": {
        "host": "0.0.0.0",
        "port": 8080,
        "workers": 1
    },
    "logging": {
        "console": {            
            "type": "Console",
            "level": "debug",
            "colorize": true
        }
    },
    "plugins": {
        "installed": [
            "defcon-rest-gateway",
            "defcon-event-log"
        ],
        "defcon-event-log": {
            "redis": {
                "host": "localhost",
                "port": 6379,
                "db": 0,
                "options": {
                    "enable_offline_queue": false
                }
            },
            "pageSize": 14,
            "pages": 10
        }
    }
}